<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.movieonlinedemo.mapper.MovieMapper">

    <select id="getMovie" resultType="java.util.HashMap">
        SELECT ID id,NAME name,PIC_URL pic_name,TYPES type FROM MOVIE WHERE DES != 'æ— ' order by score desc limit 100
    </select>

</mapper>


